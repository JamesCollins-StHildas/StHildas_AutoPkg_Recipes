<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
		<string>Download recipe for Google Backup and Sync. Finds and downloads a release of Google Backup and Sync</string>
	<key>Identifier</key>
		<string>com.github.StHildas.download.googlebackup</string>
	<key>Input</key>
	<dict>
			<key>NAME</key>
			<string>Google Backup and Sync</string>
	</dict>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>https://dl.google.com/drive/InstallBackupAndSync.dmg</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
				
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
